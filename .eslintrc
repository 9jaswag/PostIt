{
  "root": true,
  "extends": "airbnb",
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },
  "globals": {
    "document": true,
    "localStorage": true,
    "sessionStorage": true,
    "window": true,
    "jest": true,
    "expect": true
  },
  "rules": {
    "comma-dangle": 0,
    "consistent-return": 0,
    "class-methods-use-this": 0,
    "jsx-a11y/img-has-alt": 0,
    "import/no-named-as-default": 0,
    "react/forbid-prop-types": 0,
    "react/prefer-stateless-function": 0,
    "jsx-a11y/href-no-hash": 0,
    "import/no-extraneous-dependencies": 0
  }
}