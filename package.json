{
  "name": "postit",
  "version": "1.0.0",
  "description": "PostIt is a simple application that allows friends and colleagues create groups for notifications.",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon --exec babel-node ./server/bin/www",
    "start": "babel-node ./server/bin/www",
    "test": "NODE_ENV=test sequelize db:migrate && NODE_ENV=test nyc mocha --compilers js:babel-core/register server/test/userController server/test/groupController server/test/messageController server/test/helpers --timeout 50000",
    "lint": "esw server --color",
    "build-css": "node-sass --include-path scss ./template/styles/main.scss ./template/styles/style.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test:client": "jest",
    "build": "webpack --config webpack.config.prod.js",
    "postinstall": "npm run build",
    "e2e-setup": "selenium-standalone install",
    "e2e-server": "selenium-standalone start",
    "test:e2e": "nightwatch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/9jaswag/PostIt.git"
  },
  "keywords": [
    "Node",
    "Express",
    "PostgreSQL"
  ],
  "author": "Chuks Opia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/9jaswag/PostIt/issues"
  },
  "homepage": "https://github.com/9jaswag/PostIt#readme",
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "react"
    ],
    "collectCoverage": true
  },
  "dependencies": {
    "axios": "^0.16.2",
